{
  "env": {
    "jest": true
  },
  "plugins": ["prettier", "jest"],
  "extends": [
    "airbnb-typescript",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "import/prefer-default-export": "off",
    "quotes": ["error", "single"]
  }
}
